            &lt;span style=&#34;color: #e6db74&#34;&gt;&amp;#39;hTodo&amp;#39;&lt;/span&gt;&lt;span style=&#34;color: #f92672&#34;&gt;:&lt;/span&gt; &lt;span style=&#34;color: #e6db74&#34;&gt;&amp;#39;url&amp;#39;&lt;/span&gt;
